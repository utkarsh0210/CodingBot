#include <stdio.h>
void swap(int *x,int *y);
int main()
{
    int *a,*b;
    int c,m,n;
    printf("emter m=");
    scanf("%d",&m);
    a=&m;
    printf("\n enter n=");
    scanf("%d",&n);
    b=&n;
    swap(a,b);
    printf("%d \n %d",m,n);
    return 0;
}
void swap(int *x,int *y)
{
    int k;
    k=*x;
    *x=*y;
    *y=k;
}
